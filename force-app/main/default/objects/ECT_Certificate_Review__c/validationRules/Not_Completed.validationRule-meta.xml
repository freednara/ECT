<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_Completed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL( Status__c ,&quot;Completed&quot;), ISBLANK( SM_Date_Posted__c ) )</errorConditionFormula>
    <errorMessage>Certificate review not completed until Date Posted to Social Media</errorMessage>
</ValidationRule>
